# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = crlf
insert_final_newline = true
indent_style = space
indent_size = 4

[node_modules/**]
end_of_line = none
insert_final_newline = none
indent_style = none
indent_size = none

[**/node_modules/**]
end_of_line = none
insert_final_newline = none
indent_style = none
indent_size = none
